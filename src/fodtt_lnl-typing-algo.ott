Cshiftingctx :: '' ::=

  defn
  csctx ( nctx ) ( nctx' ) :: :: cs_nctx :: 'cs_nctx_' 
    {{ tex \overleftarrow{[[nctx]]} \equiv [[nctx']] }}
    {{ com C-shift on contexts }} by

  ------------------------ :: empty
  csctx( empty ) ( empty ) 

  csctx( nctx ) (nctx')
  csTy nA nA'
  --------------------------- :: var
  csctx( nctx,nA ) (nctx,nA')

