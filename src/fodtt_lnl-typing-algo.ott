CshiftingTunshifting :: 'cstu_' ::=

  defn
  cstuTy nTy nTy' :: :: nTy :: 'nTy_'
    {{ tex \overleftarrow{[[nTy]] [ 0_\Gamma / 0_T ]  } \equiv [[nTy']] }} 
    {{ com C-shifting T-unshifting on types }} by
 
  ----------- :: tcon
  cstuTy a  a

  cstuTy nA nA'
  cstuTy nB nB'
  ---------------------------------- :: pi_intro
  cstuTy (Pi nA . nB) (Pi nA' . nB')

  cstuTy nA nA'
  cstute nM nM'
  -------------------------- :: pi_elim
  cstuTy ( nA nM ) (nA' nM')

  defn
  cstute nte nte'  :: :: nte :: 'nte_'
    {{ tex \overleftarrow{[[nte]] [ 0_\Gamma / 0_T ]  } \equiv [[nte']] }} 
    {{ com C-shifting T-unshifting on terms }} by
 
  ----------- :: con
  cstute c  c

  ---------------------- :: ixc
  cstute ixc  (succ ixc)

  ------------------- :: zerot
  cstute zerot  zeroc

  ---------------------- :: suct
  cstute (suct ixt)  ixt

  cstuTy nA nA'
  cstute nM nM'
  --------------------------------- :: pi_intro
  cstute (\ nA . nM) (\ nA' . nM' )

  cstute nM nM'
  cstute nN nN'
  -------------------------- :: pi_elim
  cstute ( nM nN ) (nM' nN')

